[alias]
  who = "!echo ; echo ; echo \"\\tworking as:\" ; echo \"\\t\\t   `git config user.name`\" ; echo \"\\t\\t   `git config user.email`\" ; echo ; echo"
  what = "!git who ; git diff"
  st = status
  d = diff
  ci = commit -v
  cia = commit -v -a
  co = checkout
  cp = cherry-pick
  l = log
  ll = log -p
  lt = log trunk..
  llt = log -p trunk..
  lm = log master..
  llm = log -p master..
  b = branch
  up = pull
  rmb = !sh -c '(git branch -D $1 || test 1) && git push origin :$1' -
  parent-update = "!git fetch parent && git rebase parent/master"
  consume = !sh -c 'git merge $1 && git rmb $1' -
  hco = "!~/bin/git-hub-commit"
