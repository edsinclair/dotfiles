	authors = Michael Grosser <mgrosser@zendesk.com>
[pager]
  color = true
[core]
	editor = mvim --nofork
	excludesfile = %{HOME}/.gitignore
[color]
	diff = auto
	status = auto
	branch = auto
[push]
	default = current
[branch]
  autosetuprebase = always # rebase when pulling
